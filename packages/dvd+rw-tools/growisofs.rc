FILEVERSION 7,1,0,BUILD_NUMBER
PRODUCTVERSION 7,1,0,BUILD_NUMBER
FILEFLAGSMASK 0x17
FILEFLAGS 0
FILEOS 4
FILETYPE 1
FILESUBTYPE 0
BEGIN
